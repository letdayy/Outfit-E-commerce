{
    "extends": "eslint:recommended",
    "rules": {
      "no-console": "off",
      "semi": ["error", "always"],
      "quotes": ["error", "double"],
      "no-undef": "off"
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },

    "env": {
        "es6": true
    }
}